    type: vless
    uuid: ${uuid}
    cipher: auto
    tls: false
    skip-cert-verify: true
    servername: ${domain}
    network: ws
    ws-opts:
      path: /vless-ntls
      headers:
        Host: ${domain}
    udp: true
proxy-groups:
  - name: PAKYAVPN-AUTOSCRIPT
    type: select
    proxies:
      - XRAY_VLESS_DIGI_${user}
      - DIRECT
rules:
  - MATCH,PAKYAVPN-AUTOSCRIPT
EOF

# Move Yaml To html
mv /usr/local/etc/xray/$user-VLESS-TLS.yaml /home/vps/public_html/$user-VLESS-TLS.yaml
mv /usr/local/etc/xray/$user-VLESS-NTLS.yaml /home/vps/public_html/$user-VLESS-NTLS.yaml
mv /usr/local/etc/xray/$user-VLMAXIS.yaml /home/vps/public_html/$user-VLMAXIS.yaml
mv /usr/local/etc/xray/$user-VLDIGI.yaml /home/vps/public_html/$user-VLDIGI.yaml
clear
echo -e ""
echo -e "════════════[XRAY VLESS WS]════════════"
echo -e "Remarks           : ${user}"
echo -e "Domain            : ${domain}"
echo -e "Port TLS          : $tls"
echo -e "Port None TLS     : $none"
echo -e "ID                : ${uuid}"
echo -e "Security          : TLS"
echo -e "Encryption        : None"
echo -e "Network           : WS"
echo -e "Path TLS          : /vless-tls"
echo -e "Path None TLS     : /vless-ntls"
echo -e "═══════════════════════════════════════"
echo -e "Link WS TLS       : ${vlesslink1}"
echo -e "═══════════════════════════════════════"
echo -e "Link WS None TLS  : ${vlesslink2}"
echo -e "═══════════════════════════════════════"
echo -e "Link (MAXIS)      : ${vlesslink3}"
echo -e "═══════════════════════════════════════"
echo -e "Link (DIGI)       : ${vlesslink4}"
echo -e "═══════════════════════════════════════"
echo -e "YAML WS TLS       : http://${domain}:81/$user-VLESS-TLS.yaml"
echo -e "════════════════════════════════════════"
echo -e "YAML WS None TLS  : http://${domain}:81/$user-VLESS-NTLS.yaml"
echo -e "════════════════════════════════════════"
echo -e "YAML (MAXIS)      : http://${domain}:81/$user-VLMAXIS.yaml"
echo -e "════════════════════════════════════════"
echo -e "YAML (DIGI)       : http://${domain}:81/$user-VLDIGI.yaml"
echo -e "════════════════════════════════════════"
echo -e "Created   : $harini"
echo -e "Expired   : $exp"
echo -e "Script By $creditt"
echo ""
read -n 1 -s -r -p "Press any key to back on menu xray"
xraay
}
