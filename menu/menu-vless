    server: ${sts}${domain}
    port: ${tls}
    type: vless
    uuid: ${uuid}
    cipher: auto
    tls: true
    skip-cert-verify: true
    servername: ${sni}
    network: ws
    ws-opts:
      path: /vless-tls
      headers:
        Host: ${domain}
    udp: true
